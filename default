#!/usr/bin/python
import cgitb
cgitb.enable()

from wsgiref.handlers import CGIHandler
import os
import datetime

from flask import Flask
app = Flask(__name__)

# Get the "root" url path, because
# Flask isn't running at the domain root.
path = os.environ['PATH_INFO']
root = '/'.join(path.split('/')[0:5])

@app.route(root + "/")
def tables():
    return "{} the time is {}".format('tables', datetime.datetime.now())

@app.route(root + "/<table>")
def table(table):
    return "{} the time is {}".format(table, datetime.datetime.now())

CGIHandler().run(app)
